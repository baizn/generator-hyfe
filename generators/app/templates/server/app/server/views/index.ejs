<!DOCTYPE html>
<html>
  <head>
    <title>Redux real-world example</title>
    <% cssUrls.forEach(function(src) { %>
      <link rel="stylesheet" href="<%= src %>" type="text/css" media="screen" charset="utf-8">
    <% }) %>
  </head>
  <body>
    <div id="root"><%- html %></div>
    <script type="text/javascript" charset="utf-8">
      window.__INITIAL_STATE__ = '<%= initialState %>';
    </script>

    <% scriptsUrls.forEach(function(src){ %>
      <script src="<%= src %>"></script>
    <% }) %>

  </body>
</html>

